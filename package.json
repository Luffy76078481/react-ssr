{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "React+KOA服务端渲染基础框架",
  "main": "clientIndex.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./webpackConfig/webpack.client.config.js",
    "build:client": "rimraf clientBuild/* && cross-env NODE_ENV=production webpack --config ./webpackConfig/webpack.client.config.js --progress",
    "build:server": "rimraf serverBuild/* && webpack --config ./webpackConfig/webpack.server.config.js --progress",
    "server:dev": "nodemon --watch serverBuild --exec node \"./serverBuild/root.server.js\"",
    "dev:watch": "nodemon --watch src --exec npm run build:server"
  },
  "author": "Luffy",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.6",
